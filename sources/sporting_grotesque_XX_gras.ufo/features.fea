feature case {
# automatic
sub acute by acute.case;
sub circumflex by circumflex.case;
sub dieresis by dieresis.case;
sub grave by grave.case;
sub ring by ring.case;
sub tilde by tilde.case;
} case;

feature liga {
# automatic
sub f i by fi;
sub f l by fl;
sub T T by T_T.liga;
sub f t by f_t.liga;
} liga;

feature salt {
# automatic
sub K by K.ss01;
sub O by O.ss01;
sub Q by Q.ss01;
sub R by R.ss01;
sub S by S.ss01;
sub Udieresis by Udieresis.ss01;
sub periodcentered by periodcentered.ss01;
sub braceleft by braceleft.ss01;
sub braceright by braceright.ss01;
sub bracketleft by bracketleft.ss01;
sub bracketright by bracketright.ss01;
sub parenleft by parenleft.ss01;
sub parenright by parenright.ss01;
sub greater by greater.ss01;
sub less by less.ss01;
sub northEastArrow by northEastArrow.ss01;
} salt;

feature ss01 {
# automatic
sub K by K.ss01;
sub O by O.ss01;
sub Q by Q.ss01;
sub R by R.ss01;
