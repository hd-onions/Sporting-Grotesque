sub Ograve by Ograve.smcp;
sub Oslash by Oslash.smcp;
sub OE by OE.smcp;
sub P by P.smcp;
sub Q by Q.smcp;
sub R by R.smcp;
sub S by S.smcp;
sub T by T.smcp;
sub U by U.smcp;
