sub Ucircumflex by Ucircumflex.smcp;
sub Udieresis by Udieresis.smcp;
sub Ugrave by Ugrave.smcp;
sub V by V.smcp;
sub W by W.smcp;
sub X by X.smcp;
sub Y by Y.smcp;
sub Yacute by Yacute.smcp;
sub Ydieresis by Ydieresis.smcp;
sub Z by Z.smcp;
